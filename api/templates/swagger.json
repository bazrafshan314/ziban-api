{
    "openapi": "3.0.0",
    "info": {
        "title": "Ziban internal api documentation"
    },
    "paths": {
        "/users/": {
            "get": {
                "summary": "get a list of all users"
            }
        },
        "/users/create/": {
            "post": {
                "summary": "create a new user",
                "parameters": [
                    {
                        "name": "username",
                        "location": "query",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "email",
                        "location": "query",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "password",
                        "location": "query",
                        "schema": {
                            "type": "string"
                        }
                    }
                ]
            }
        }
    },
    "servers": [
        {
            "url": "/api"
        }
    ]
}
